	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#7c8aa3",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#C4A1F8",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"string",
				"constant.other.symbol",
				"string.quoted",
				"string.regexp"
			],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["variable", "variable.other.readwrite", "variable.parameter"],
			"settings": {
				"foreground": "#E6ECF5"
			}
		},
		{
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": ["entity.name.type", "support.class", "support.type"],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": ["entity.name.tag", "meta.tag.sgml", "meta.tag.html"],
			"settings": {
				"foreground": "#7BB8F0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"meta.selector.css",
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#5AA3E6"
			}
		},
		{
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss"
			],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": ["variable.scss", "variable.other.sass"],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["entity.name.function.scss", "support.function.mixin.scss"],
			"settings": {
				"foreground": "#F87979"
			}
		},
		{
			"scope": ["variable.other.readwrite.js", "identifier.js"],
			"settings": {
				"foreground": "#E6ECF5"
			}
		},
		{
			"scope": [
				"punctuation.section.block.begin",
				"punctuation.section.block.end",
				"punctuation.section.array.begin",
				"punctuation.section.array.end",
				"punctuation.definition.block",
				"punctuation.definition.array",
				"punctuation.section.parens.begin",
				"punctuation.section.parens.end",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.definition.begin",
				"punctuation.definition.end"
			],
			"settings": {
				"foreground": "#7BB8F0"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#E6ECF5",
		"parameter": "#F8BB77",
		"property": "#E6ECF5",
		"function": "#7BB8F0",
		"method": "#7BB8F0",
		"class": "#A78DEA",
		"interface": "#A78DEA",
		"enum": "#F87979",
		"typeParameter": "#A78DEA",
		"namespace": "#7BB8F0",
		"macro": "#F8BB77",
		"modifier": "#C4A1F8",
		"regexp": "#7ADCA0",
		"decorator": "#F87979",
		"punctuation": "#7BB8F0"
	}
}
